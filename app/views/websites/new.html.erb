<h1>New Website</h1>
<%= flash_notice %>

<div class="form">
	<%= error_messages_for :page -%>

	<fieldset>
		<legend>Add a new website</legend>
    <% form_for @website do |form| %>
    <%= render :partial => 'form', :object => form %>

    <%= submit_tag "Create New Website" %>

  <% end %>
  </fieldset>
</div>