<h1>Edit Page</h1>
<%= flash_notice %>

<div class="form">
	<%= error_messages_for :page -%>

	<fieldset>
		<legend>Edit page</legend>
    <% form_for :page, :url => { :action => "update", :id => @page.id } do |form| %>
    <%= render :partial => 'form', :object => form %>

    <%= submit_tag "Save" %>

  <% end %>
  </fieldset>
</div>