<h1>New Forum</h1>
<%= flash_notice %>

<div class="form">
	<%= error_messages_for :forum -%>

	<fieldset>
		<legend>Add a new forum</legend>
    <%= form_for :forum, @forum, :url => { :action => "create" } do |form| %>
    <%= render :partial => 'form', :object => form %>

    <%= submit_tag "Create New Forum" %>

  <% end %>
  </fieldset>
</div>
